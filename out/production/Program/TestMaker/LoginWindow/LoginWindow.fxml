<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<Pane fx:id="login_pane" prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TestMaker.LoginWindow.LoginWindowController">
   <children>
      <TextField fx:id="login_text_field" layoutX="107.0" layoutY="86.0" promptText="USER NAME">
         <cursor>
            <Cursor fx:constant="TEXT" />
         </cursor></TextField>
      <TextField fx:id="password_text_field" layoutX="107.0" layoutY="141.0" prefHeight="31.0" prefWidth="186.0" promptText="PASSWORD">
         <cursor>
            <Cursor fx:constant="TEXT" />
         </cursor></TextField>
      <Label fx:id="login_label" layoutX="107.0" layoutY="65.0" text="User name" />
      <Label fx:id="password_label" layoutX="107.0" layoutY="120.0" text="Password" />
      <Button fx:id="login_button" alignment="CENTER" layoutX="130.0" layoutY="205.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="141.0" text="Log in" />
      <Label fx:id="invalid_data_label" layoutX="84.0" layoutY="37.0" text="Invalid user name and/or password" textAlignment="CENTER" textFill="RED" underline="true" />
   </children> 
</Pane>
